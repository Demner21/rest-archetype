[*.java]
indent_style=space
indent_size=2
#tab_width=4
end_of_line=cr
trim_trailing_whitespace=true
insert_final_newline=true

[Jenkinsfile]
indent_style=tab
tab_width=3
end_of_line=cr
trim_trailing_whitespace=true
insert_final_newline=true
